; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[  ]
_global_script_class_icons={

}

[application]

config/name="Choose Food for Delivery Service"
run/main_scene="res://Main.tscn"
config/icon="res://assets/Foods/burger.png"

[autoload]

Global="*res://Global.gd"

[display]

window/size/width=750
window/size/height=1334
window/size/borderless=true
window/size/fullscreen=true
window/size/test_width=325
window/size/test_height=617
window/handheld/orientation="portrait"
window/stretch/mode="viewport"
window/stretch/aspect="expand"

[input_devices]

pointing/emulate_touch_from_mouse=true

[rendering]

quality/driver/driver_name="GLES2"
vram_compression/import_bptc=true
vram_compression/import_etc=true
vram_compression/import_pvrtc=true
gles2/compatibility/disable_half_float=true
gles2/compatibility/enable_high_float.Android=true
environment/default_environment="res://default_env.tres"
